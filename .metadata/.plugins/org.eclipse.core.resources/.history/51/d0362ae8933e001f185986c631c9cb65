package pw_skills;

public class inverted_alphabet_triangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch='A';
		int n=5;
		for(int i=n;i>=1;i--) {
			for(int j=1;j<i;j++) {
				System.out.print(ch+" ");
				ch--;
			}
			System.out.println();
		}

	}

}
