package pw_skills;

public class star_plus {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=5;
		//int mid=n/2+1;
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				if(i==2 || j==2) {
					System.out.print("* ");
				}else { 
					System.out.print(" ");
				}
			}
			System.out.println();
		}

	}

}
