package pw_skills;

public class star_plus {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=4;
		int mid=n/2+1;
		for(int i=1;i<n;i++) {
			for(int j=1;j<n;j++) {
				if(i==mid || j==mid) {
					System.out.print("* ");
				}else 
					System.out.println();
			}
		}

	}

}
